<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="tarmii.theme"
      lang="en">

<body>
    <metal:main fill-slot="content-title">
    <h1 class="documentFirstHeading" i18n:translate="Activities`-with-broken-images">
        Activities with broken images
    </h1>
    </metal:main>
    <metal:main fill-slot="content-core">
    <div tal:repeat="item view/images">
        <a href="#" tal:attributes="href python:''.join(item.getPhysicalPath())">
            <span tal:replace="item/getId" />
        </a>
    </div>
    </metal:main>
</body>
</html>
